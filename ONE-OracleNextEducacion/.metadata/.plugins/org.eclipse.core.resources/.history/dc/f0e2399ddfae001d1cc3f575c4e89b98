
public class TestReferencia {
	public static void main(String[] args) {
		Cliente user = new Cliente();
		user.setNombre("Daniel");
		user.documento = "1144208104";
		user.telefono = "3104189457";
		
		Cuenta cuentaUser = new Cuenta(); 
		cuentaUser.titular = user;
		cuentaUser.setAgencia(1);
		cuentaUser.depositar(1000);;
		
		System.out.println(cuentaUser.titular);
		System.out.println(user);
	}
}
